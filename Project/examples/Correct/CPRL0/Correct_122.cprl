// test relational expressions on boolean and character values
// output should be as follows:
// false < true
// 'a' < 'b'

proc main()
  {
    if false < true then
        writeln "false < true";
    else
        writeln "false >= true";

    if 'a' < 'b' then
        writeln "'a' < 'b'";
    else
        writeln "'a' >= 'b'";
  }
