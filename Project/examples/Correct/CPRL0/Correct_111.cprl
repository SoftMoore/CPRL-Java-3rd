// Correct output is as follows:
//      x = 5
//      y = 2
//  x + y = 7
//  x - y = 3
//    x*y = 10
//    x/y = 2
//x mod y = 1

const TWO := 2;
var x : Integer := 5;
var y : Integer := TWO;

proc main()
  {
    writeln "      x = ", x;
    writeln "      y = ", y;
    writeln "  x + y = ", x + y;
    writeln "  x - y = ", x - y;
    writeln "    x*y = "  , x*y;
    writeln "    x/y = "  , x/y;
    writeln "x mod y = ", x mod y;
  }
