// Correct output is as follows:
// m = 8, n = 6, k = 2
// x = 8, y = 6

proc p(var m : Integer, n : Integer)
  {
    var k : Integer := 2;

    m := n + k;
    writeln "m = ", m, ", n = ", n, ", k = ", k;
  }

proc main()
  {
    var x, y : Integer;
    x := 5;
    y := 6;
    p(x, y);
    writeln "x = ", x, ", y = ", y;
  }
