// Output should be:
// max(51, 3) = 51
// max(9, 9) = 9
// max(-1, 7) = 7
// max(-1, -5) = -1

proc main()
  {
    writeln "max(51, 3) = ",  max(51, 3);
    writeln "max(9, 9) = ",   max(9, 9);
    writeln "max(-1, 7) = ",  max(-1, 7);
    writeln "max(-1, -5) = ", max(-1, -5);
  }

fun max(x : Integer, y : Integer) : Integer
  {
    if x >= y then
        return x;
    else
        return y;
  }
