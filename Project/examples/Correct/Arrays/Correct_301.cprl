const arraySize := 10;

type A = array[arraySize] of Integer;

var i : Integer;
var a : A;

proc main()
  {
    // initialize array
    i := 0;
    while i < arraySize loop
      {
        a[i] := 2*i;
        i := i + 1;
      }

    // print values in the array
    i := 0;
    while i < arraySize loop
      {
        writeln a[i];
        i := i + 1;
      }
  }
